@using WebTIGA;
@model WebTIGA.Models.ContenedorModelos
<html>
<head>
    <script type="text/javascript" class="init">
        $(document).ready(function () {
            $('#example1').DataTable({
                dom: 'Bfrtip',
                buttons: [
                    'excelHtml5',
                ]
            });
        });
    </script>
</head>
<body>
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-info">DETALLE EQUIPOS-FASE</h6>
        </div>
        <div class="card-body">
            <div class="col-lg-12">

            </div>

            <div class="card-body">
                <div class="table-responsive">
                    <table id="example1" class="display nowrap" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th style="text-align:center">Equipo</th>
                                <th style="text-align:center">Fase</th>
                                <th style="text-align:center">Total Horas </th>
                                <th style="text-align:center">Dias Auditor</th>


                            </tr>
                        </thead>

                        <tbody>
                            @foreach (var file in Model.SP_TT_EQUIPO_ETAPAS)
                            {
                                <tr>
                                    <td style="text-align:initial">
                                        @file.Equipo
                                    </td>
                                    <td style="text-align:center">
                                        @file.Fase
                                    </td>
                                    <td style="text-align:center">
                                        @file.Total_Horas
                                    </td>
                                    <td style="text-align:center">
                                        @file.Días_Auditor
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</body>
</html>

