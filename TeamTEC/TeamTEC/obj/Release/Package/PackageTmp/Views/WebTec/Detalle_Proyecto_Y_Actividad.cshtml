@using TeamTEC;
    @model TeamTEC.Models.ContenedorModelos

<html>
<head>
    <script type="text/javascript" class="init">
        $(document).ready(function () {
            $('#example1').DataTable({
                dom: 'Bfrtip',
                buttons: [
                    'excelHtml5',
                ]
            });
        });
    </script>
</head>
    <body>
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-info">DETALLE PROYECTO-ACTIVIDAD</h6>
            </div>
            <div class="card-body">
                <div class="col-lg-12">
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table  id="example1" class="display nowrap" width="100%" @*cellspacing="0"*@>
                            <thead>
                                <tr>
                                    <th>Codigo</th>
                                    <th>Titulo</th>
                                    <th>Fase </th>
                                    <th>Actividades</th>
                                    <th>Horas</th>

                                </tr>
                            </thead>

                            <tbody>
                                @foreach (var file in Model.SP_TT_DETALLE_PROYECTO_ACTIVIDAD)
                                {
                                    <tr>
                                        <td>
                                            @file.Code
                                        </td>
                                        <td>
                                            @file.Title
                                        </td>
                                        <td>
                                            @file.Fase
                                        </td>
                                        <td>
                                            @file.Actividades
                                        </td>
                                        <td>
                                            @file.Horas
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </body>
    </html>
